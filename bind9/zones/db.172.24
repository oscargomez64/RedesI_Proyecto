;
; BIND data file for local loopback interface
;
$TTL	604800
@		IN		SOA		prored.local. 	admin.prored.local. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@		IN		NS		prored.local.
3       IN      PTR     prored.local.
4		IN		PTR		sitio1.prored.local.
4		IN		PTR		sitio2.prored.local.
4		IN		PTR		sitio3.prored.local.
